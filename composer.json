{
  "name": "yiisoft/persisthandler",
  "description": "Persistent Handler Facebook SDK",
  "keywords": ["yii2", "facebook", "handler"],
  "homepage": "",
  "type": "project",
  "license": "BSD-3-Clause",
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "danfercf/persistenthandler",
        "version": "1",
        "source": {
          "url": "git://github.com/danfercf1/persistenthandler.git",
          "type": "git",
          "reference": "master"
        },
        "autoload": {
          "psr-0" : {
            "danfercf\\persistentHandler" : "src"
          }
        }
      }
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": ">=2.0.6",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "facebook/graph-sdk": "^5.3"
  },
  "config": {
    "process-timeout": 1800
  }
}
